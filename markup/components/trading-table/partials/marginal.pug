-
  var data = [{
      title: 'Символ',
      key: 'symbol',
      sortable: true,
      tooltip: true,
      tooltipTitle: 'Договор, являющийся производным финансовым инструментом, обязанность сторон по которому зависит от изменения курса соответствующей валютной пары',
  },{
      title: 'Кредитное плечо',
      key: 'funds50k',
      sortable: true,
      tooltip: true,
      tooltipTitle: 'Значение кредитного плеча является фиксированным',
  },{
      title: 'Уровень Stop out',
      key: 'funds50k_stopOutLevel',
      sortable: true,
      tooltip: true,
      tooltipTitle: 'Уровень stop out активируется по первой рыночной цене при уровне маржи ниже заданного или равного ему',
  },{
      title: 'Обеспечение в валюте счета',
      shortTitle: 'Обеспечение',
      key: 'margin50k',
      sortable: true,
      tooltip: true,
      tooltipTitle: 'Минимальное количество денежных средств для открытия сделки заданного объема',
  }];

+b.trading-table.marginal-table
  +e.TABLE.table
    +e.THEAD.head
      +e.TR.head-row
        each cell in data
          +e.TH.head-cell(data-column-key=cell.key data-tooltip=cell.tooltip, data-title=cell.tooltipTitle data-sortable=cell.sortable)
            +e.SPAN.head-cell-title
              if cell.shortTitle
                span.normal=cell.title
                span.short=cell.shortTitle
              else
                | #{cell.title}

              if cell.tooltip
                +e.SPAN.tooltip-mark
            if cell.sortable
              +e.sort
                include ../../../static/img/svg/sort.svg
    +e.TBODY.body
